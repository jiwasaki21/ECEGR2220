.data

z: .word 2
i: .word

.text

main:
	lw t1, z
	lw t2, i
	li a1, 20
	li a2, 100
	
	li t2, 0
	beq t2, x0, for
	
	for:
	ble t2, a1, nextcheck
	bgt t2, a1, end
	
	nextcheck:
	addi t2, t2, 2
	addi t1, t1, 1
	blt t2, a2, while1
	
	while1:
	blt t2, a2, do
	
	while2:
	bgt t2, x0, do2
	
	do:
	addi t1, t1, 1
	j while2
	
	do2:
	addi t1, t1, -1
	addi t2, t2, -1
	j for
	
	end:
	sw t1, z, t6
	li a7, 1
	lw a0, z
	ecall